<Window x:Class="CarCostNotepad.View.PopupWindows.MainObjectCreator"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarCostNotepad.View.PopupWindows"
        mc:Ignorable="d" Background="DimGray"
        Title="MainObjectCreator" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <ComboBox Name="Types">

                <ComboBox.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="{Binding Type}"/>
                    </DataTemplate>
                </ComboBox.ItemTemplate>

            </ComboBox>
            <Frame Background="DimGray" x:Name="MainFrame" />
        </StackPanel>
        
    </Grid>
</Window>
